<!doctype html>


<html>
<head>

  <title>Progress Tracker</title>   
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
  <link href="css/bootstrap.min.css" rel="stylesheet">
  <link href="css/bootstrap-theme.min.css" rel="stylesheet">
  <!-- Materialize Template stuff (Material Design) coding thing-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link type="text/css" rel="stylesheet" href="css/materialize.min.css"  media="screen,projection"/>
  <!--our css-->  
  <!--<link href="css/progressTracker.css" rel="stylesheet" >-->
  <link href="css/PTmat.css" rel="stylesheet">
  
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
  <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->        
 
  <link type="text/css" rel="stylesheet" href="clockAssets/materialize/css/materialize.min.css">
  <link type="text/css" rel="stylesheet" href="clockAssets/css/styles.css">
  <link type="text/css" rel="stylesheet" href="clockAssets/jonthornton-jquery-timepicker/jquery.timepicker.css">



</head>



<body>

     <!--Materialize Navbar Code-->
  <div class="navbar-fixed">
  <nav class="teal"> <!--change navbar background color here!-->
    <div class="nav-wrapper">
      <a href="/" class="brand-logo">Progress Tracker</a>
      <a href="#" data-activates="mobile-demo" class="button-collapse">
        <i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down"> <!-- full web page -->
        {{#each navLinks}}
          <li><a href="{{link}}">{{linkText}}</a></li>
        {{/each}}
      </ul>
      <ul class="side-nav" id="mobile-demo"> <!-- mobile -->
        <br>
        <br>
        {{#each navLinks}}
          <li><a href="{{link}}">{{linkText}}</a></li>
        {{/each}}
      </ul>
    </div>
  </nav>
  </div>  




    <div class="wrapper">

        <div class="container timer">
            <form>
                <div class="control input-field">
                    <input id="timer-input" type="number">
                    <label for="timer-input">Minutes</label>
                </div>

                <a class="timer-btn start waves-effect"><i class="mdi-av-play-arrow"></i></a>
                <a class="timer-btn pause waves-effect"><i class="mdi-av-pause"></i></a>
                <a class="timer-btn reset waves-effect"><i class="mdi-av-replay"></i></a>

                <div class="control checkbox">
                    <input type="checkbox" id="timer-sounds" />
                    <label for="timer-sounds">Sounds</label>
                </div>

            </form>

            <div class="clock inactive z-depth-1 waves-effect">0:00</div>
        </div>

        <div class="container alarm">
            <form>

                <div class="control input-field">
                    <input id="alarm-input" type="text" class="validate">
                    <label for="alarm-input">Set Time</label>
                </div>

                <div class="control switch">
                    <label>
                        Off
                        <input type="checkbox" disabled id="alarm-btn">
                        <span class="lever"></span>
                        On
                    </label>
                </div>

                <div class="control checkbox">
                    <input type="checkbox" id="alarm-sounds" />
                    <label for="alarm-sounds">Sounds</label>
                </div>

            </form>

            <div class="clock z-depth-1 inactive">
                <span>Not Set</span>
                <div class="overlay waves-effect"></div>
            </div>
        </div>

        <div class="container stopwatch">
            <form>
                <a id="stopwatch-btn-start" class="waves-effect waves-teal btn-flat">Start</a>
                <a id="stopwatch-btn-pause" class="waves-effect waves-teal btn-flat">Pause</a>
                <a id="stopwatch-btn-reset" class="waves-effect waves-teal btn-flat">Reset</a>
            </form>

            <div class="clock inactive z-depth-1">
                <span>0:00:00.0</span>
                <div class="overlay waves-effect"></div>
            </div>
        </div>

        

    </div>

  <!-- Javascripts -->
  <script src="https://code.jquery.com/jquery.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <!-- Materialize js-->
  <script type="text/javascript" src="js/materialize.min.js"></script>
  <!-- own js-->
  <script src="js/progressTracker.js"></script>


  <script src="clockAssets/jonthornton-jquery-timepicker/jquery.timepicker.min.js"></script>

  <script src="clockAssets/js/hammer.js"></script>
  <script src="clockAssets/js/script.js"></script>
  <script src="clockAssets/js/alarm.js"></script>
  <script src="clockAssets/js/stopwatch.js"></script>
  <script src="clockAssets/js/timer.js"></script>
    <script src="js/progressTracker.js"></script>



  
</body>
</html>
